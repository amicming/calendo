<h3>Sign In</h3>
<%= form_for(:session, url: sessions_path) do |f| %>
	<%= f.label :email %>
	<%= f.text_field :email %>

	<%= f.label :password %>
	<%= f.password_field :password %><br />

	<%= f.submit "Sign in", class: "btn btn-small btn-primary" %>
<% end %>
New User? <%= link_to "Sign up now!", signup_path, class: "btn btn-mini btn-primary" %>